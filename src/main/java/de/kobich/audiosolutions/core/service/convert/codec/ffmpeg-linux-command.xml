<command-definition>
	<!-- ffmpeg -i input.m4a -c:v copy -c:a libmp3lame -q:a 4 output.mp3 -->
	<command>ffmpeg</command>
	<!-- input file -->
	<param id="source">-i</param>
	<param id="source">{0}</param>
	
	<param id="codec">-c:a</param>
	<param id="codec">{0}</param>
	
	<!-- Control quality with -qscale:a (or the alias -q:a). Values are encoder specific. -->
	<param id="quality">-q:a</param>
	<param id="quality">{0}</param>
	<param id="quality.aac">-vbr</param>
	<param id="quality.aac">{0}</param>
	
	<param id="target">{0}</param>
</command-definition>

